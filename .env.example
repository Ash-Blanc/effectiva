# Effectiva Environment Variables
# Copy this file to .env and fill in your actual values

# Required: Google Gemini API Key
# Get your key from: https://aistudio.google.com/app/apikey
GOOGLE_API_KEY=your_google_api_key_here

# Optional: OpenAI API Key (for future use with other models)
# OPENAI_API_KEY=your_openai_api_key_here

# Optional: Custom memory database path
# MEMORY_DB_PATH=/path/to/custom/effectiva_memory.db

# Optional: Custom port for AgentUI (default: 3000)
# PORT=3000

# Optional: Enable debug mode
# DEBUG_MODE=false
